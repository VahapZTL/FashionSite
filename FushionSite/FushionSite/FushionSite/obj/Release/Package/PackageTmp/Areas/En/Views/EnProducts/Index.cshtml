@{
    ViewBag.Title = "Products";
    Layout = "~/Areas/En/Views/FrontLayout.cshtml";
}
<script>
    window.onload = function () {
        $("#productLi").addClass("active");
        $("#homeLi").removeClass("active");
    }
</script>
<section class="page">


    <div class="page-bg"></div>
    <div class="container">
        <div class="page-info">
            <div class="page-title">
                Products
            </div>
            <div class="divider-half"></div>
            <div class="alt-sayfa">
                <ul>
                    <li><a href=""></a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="uruns" style="margin: 20px 0;">
                @foreach (var item in (IEnumerable<KagiEntityLibrary.GalleryEntiy.GalleryTable>)ViewData["sendProduct"])
                {
                    <div class="urun">
                        <a href="/Content/Images/Gallery/@item.ImageUrl" class="fancybox" data-fancybox-group="urun">
                            <img src="/Content/Images/Gallery/@item.ImageUrl" style="height: 100%;" alt="Kagi Moda">
                        </a>
                    </div>
                }
               

            </div>
        </div>
    </div>



</section>
