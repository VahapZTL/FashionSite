@{
    ViewBag.Title = "Referances";
    Layout = "~/Areas/En/Views/FrontLayout.cshtml";
}
<script>
    window.onload = function () {
        $("#referancesLi").addClass("active");
        $("#homeLi").removeClass("active");
    }
</script>
<section class="page">


    <div class="page-bg"></div>
    <div class="container">
        <div class="page-info">
            <div class="page-title">
                Referances
            </div>
            <div class="divider-half"></div>
            <div class="alt-sayfa">
                <ul>
                    <li><a href=""></a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="single">
            <p>
                @try
                {
                    @Html.Raw(Model.ArticleContent)
                }
                catch (Exception)
                {

                    <b>No Content!</b>
                }
               
            </p>
        </div>
    </div>



</section>

