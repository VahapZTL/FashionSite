@{
    ViewBag.Title = "Galeri";
    Layout = "~/Views/FrontLayout.cshtml";
}
<script>
    window.onload = function () {
        $("#galleryLi").addClass("active");
        $("#homeLi").removeClass("active");
    }
</script>
<section class="page">


    <div class="page-bg"></div>
    <div class="container">
        <div class="page-info">
            <div class="page-title">
                Galeri
            </div>
            <div class="divider-half"></div>
            <div class="alt-sayfa">
                <ul>
                    <li id="factoryLi"><a href="/fabrika">Fabrika</a></li>
                    <li id="productionLi"><a href="/uretim">Üretim</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="uruns" style="margin: 20px 0;">
                @foreach (var item in (IEnumerable<KagiEntityLibrary.GalleryEntiy.GalleryTable>)ViewData["sendGallery"])
                {
                    <div class="urun">
                        <a href="/Content/Images/Gallery/@item.ImageUrl" class="fancybox" data-fancybox-group="urun">
                            <img src="/Content/Images/Gallery/@item.ImageUrl" style="height:100% " alt="@item.Title">
                        </a>
                    </div>
                }
             





            </div>
        </div>
    </div>



</section>
